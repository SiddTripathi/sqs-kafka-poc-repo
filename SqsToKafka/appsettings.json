{
  "Kafka": {
    "BootstrapServers": "redpanda:9092", //change to localhost:9092 when running locally. Change to redpanda for docker
    "Acks": "all",
    "MessageTimeoutMs": 30000,
    "DefaultTopic": "sqs.to.kafka.dev"
  },
  "Sqs": {
    "Profile": "", //profile should be empty while running in docker. This 'sqs-dev' only works while running it locally
    "Region": "ap-southeast-2",
    "QueueUrl": "https://ap-southeast-2.queue.amazonaws.com/184979630605/sqs-to-kafka-dev",
    "WaitTimeSeconds": 10,
    "MaxMessages": 5,
    "VisibilityTimeoutSeconds": 30,
    "EmptyWaitDelayMs": 2000
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Routing": {
    "BodyField": "event",
    "Map": {
      "payment.succeeded": "routed.a",
      "order.cancelled": "routed.b"
    }
  }
}
